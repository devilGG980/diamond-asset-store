  "scripts": {
    "prestart": "node scripts/gen-backgrounds.js \"THUMBNAIL BACKGROUND\" || exit 0",
    "start": "concurrently \"npm run server\" \"craco start\"",
    "prebuild": "node scripts/gen-backgrounds.js \"THUMBNAIL BACKGROUND\" && npm run generate-sitemap || exit 0",
    "build": "set GENERATE_SOURCEMAP=false && craco build && node scripts/clean-build.cjs",
    "postbuild": "node scripts/inject-version.js",
    "build:netlify": "craco build",
    "test": "craco test",
    "analyze": "set ANALYZE=true && craco build",
    "optimize-images": "node scripts/optimize-images.js",
    "compress-images": "node scripts/compress-all.cjs",
    "convert-to-webp": "node scripts/convert-to-webp.cjs",
    "apply-cdn-urls": "node scripts/apply-cdn-urls.cjs",
    "upload-to-mega": "node scripts/upload-to-mega-cli.cjs",
    "generate-sitemap": "node scripts/generate-sitemap.js",
    "index-pages": "node scripts/index-api.js",
    "gen:backgrounds": "node scripts/gen-backgrounds.js \"THUMBNAIL BACKGROUND\"",
    "serve": "node scripts/local-server.js",
    "serve:build": "npm run build && npm run serve",
    "server": "ts-node server.ts",
    "dev": "concurrently \"npm run server\" \"npm start\"",
    "eject": "react-scripts eject",
    "deploy:optimized": "node scripts/apply-cdn-urls.cjs && npm run build"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
